{
  "manifest_version": 2,
  "name": "webXtension",
  "version": "1.0",
  "description": "POST request to add stress marks to russian web pages",

"permissions": [
  "http://127.0.0.1/*"
  ],

"content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]

}